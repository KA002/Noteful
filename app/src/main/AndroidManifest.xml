<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity
            android:name=".MarksActivity"
            android:exported="false"
            android:label="MarksActivity"
            android:parentActivityName=".ToolsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ToolsActivity"/>
        </activity>
        <activity
            android:name=".WebmarkActivity"
            android:exported="false"
            android:label="@string/title_activity_webmark"
            android:parentActivityName=".ToolsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ToolsActivity"/>
        </activity>
        <activity
            android:name=".ToolsActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DocActivity"
            >

        </activity>
    </application>

</manifest>